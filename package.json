{
    "name": "composaic-demo",
    "version": "0.1.1",
    "description": "Example projects for Composaic",
    "repository": {
        "type": "git",
        "url": "https://github.com/jarecsni/composaic-demo.git"
    },
    "scripts": {
        "api": "node ./server.js",
        "serve": "http-server dist -p 3000 --cors -c-1",
        "dev:all": "concurrently \"npm run dev\" \"npm run api\"",
        "webpack-help": "webpack --help",
        "start": "webpack serve --progress --mode development",
        "clean": "rm -rf ./build",
        "build": "npm run clean && webpack --progress --mode production",
        "lint": "eslint src --ext .js,.ts,.tsx --ignore-pattern '!.*.js' --no-error-on-unmatched-pattern",
        "lint:fix": "npm lint --fix",
        "prettier": "npm run format",
        "format": "prettier --config .prettierrc.js --check src/**/*.{js,jsx,ts,tsx}",
        "format:fix": "prettier --config .prettierrc.js --write src/**/*.{js,jsx,ts,tsx}",
        "type": "./node_modules/.bin/tsc --project tsconfig.json --noEmit",
        "lint-staged": "lint-staged",
        "husky-install": "husky install",
        "test": "jest"
    },
    "keywords": [
        "nodejs",
        "package",
        "npm",
        "javascript",
        "typescript"
    ],
    "author": {
        "name": "Johnny Jarecsni",
        "email": "johnny.jarecsni@icloud.com"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.21.5",
        "@babel/core": "^7.21.5",
        "@babel/plugin-syntax-import-attributes": "^7.24.7",
        "@babel/preset-env": "^7.21.5",
        "@babel/preset-typescript": "^7.21.5",
        "@eslint/compat": "^1.0.3",
        "@eslint/js": "^9.4.0",
        "@types/jest": "^29.5.13",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^8.5.0",
        "@typescript-eslint/parser": "^8.5.0",
        "babel-jest": "^29.7.0",
        "babel-plugin-module-resolver": "^5.0.2",
        "concurrently": "^9.0.1",
        "css-loader": "^7.1.2",
        "esbuild-loader": "^4.2.2",
        "eslint": "^9.12.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.2",
        "globals": "^15.3.0",
        "html-webpack-plugin": "^5.6.0",
        "http-server": "^14.1.1",
        "jest": "^29.7.0",
        "mini-css-extract-plugin": "^2.9.1",
        "prettier": "^3.3.1",
        "sass": "^1.77.8",
        "sass-loader": "^16.0.1",
        "typescript": "^5.6.2",
        "typescript-eslint": "^8.8.0",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0",
        "webpack-sources": "^3.2.3"
    },
    "dependencies": {
        "@babel/plugin-transform-react-jsx": "^7.25.2",
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@module-federation/enhanced": "^0.8.1",
        "@module-federation/runtime": "^0.6.10",
        "@mui/icons-material": "^5.16.4",
        "@mui/material": "^5.16.4",
        "composaic": "^0.10.4",
        "express": "^4.19.2",
        "mitt": "^3.0.1",
        "module-federation-import-remote": "^1.1.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.25.0",
        "runtypes": "^6.7.0"
    },
    "jest": {
        "modulePathIgnorePatterns": [
            "/node_modules/",
            "/lib/",
            "/dist/"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/fileMock.js",
            "\\.(css|less|scss)$": "<rootDir>/test/mocks/styleMock.js",
            "^(\\.{1,2}/.*)\\.js$": "$1"
        },
        "transform": {
            "^.+\\.[t|j]sx?$": "babel-jest"
        }
    }
}
